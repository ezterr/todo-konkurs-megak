{
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "rules": {
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "max-len": ["error", { "code": 120 }],
    "lines-between-class-members": "off"
  }
}
